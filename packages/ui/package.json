{
  "name": "@truststack/ui",
  "version": "0.0.13",
  "description": "Trust Stack UI library",
  "type": "module",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "react-native-import": "./dist/esm/index.native.js",
      "react-native": "./dist/cjs/index.native.js",
      "types": "./types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs",
  "module": "dist/esm",
  "source": "src/index.tsx",
  "types": "./types/index.d.ts",
  "scripts": {
    "build": "tamagui-build",
    "clean": "tamagui-build clean",
    "publish": "npm publish --access public",
    "storybook": "storybook dev -p 6006 --no-open",
    "watch": "tamagui-build --watch"
  },
  "dependencies": {
    "@floating-ui/react": "0.27.4",
    "@hookform/resolvers": "^4.1.3",
    "@tamagui/animations-react-native": "1.125.17",
    "@tamagui/core": "1.125.17",
    "@tamagui/helpers-icon": "1.125.17",
    "@tamagui/lucide-icons": "1.125.17",
    "@tamagui/shorthands": "1.125.17",
    "@tamagui/themes": "1.125.17",
    "@tanstack/react-table": "^8.16.0",
    "change-case": "^4.1.2",
    "date-fns": "^2.28.0",
    "html5-qrcode": "^2.3.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.54.2",
    "react-native": "0.74.5",
    "react-native-safe-area-context": "^5.3.0",
    "react-native-svg": "15.2.0",
    "react-native-web": "~0.19.6",
    "react-native-webview": "^13.13.2",
    "tamagui": "1.125.17",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^8.3.5",
    "@storybook/addon-essentials": "^8.3.5",
    "@storybook/addon-interactions": "^8.3.5",
    "@storybook/addon-links": "^8.3.5",
    "@storybook/addon-react-native-web": "^0.0.26",
    "@storybook/react": "^8.3.5",
    "@storybook/react-vite": "^8.3.5",
    "@storybook/test": "^8.3.5",
    "@tamagui/build": "latest",
    "@tamagui/vite-plugin": "latest",
    "@types/react": "^18.2.0",
    "@vitejs/plugin-react": "^4.3.1",
    "raf": "^3.4.1",
    "storybook": "^8.3.5",
    "tamagui-loader": "1.125.17",
    "vite": "^4.2.0",
    "vite-plugin-svgr": "^4.2.0"
  },
  "removeSideEffects": "true",
  "tamagui": {
    "build": {
      "skipEnvToMeta": true,
      "bundle.native": "./src/index.ts",
      "bundle.native.test": "./src/index.ts"
    }
  }
}