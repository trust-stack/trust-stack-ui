{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                "storybook-static/**",
                "types/**"
            ]
        },
        "publish": {
            "dependsOn": [
                "build"
            ],
            "outputs": []
        }
    }
}